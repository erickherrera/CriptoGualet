{
  "configurations": [
    {
      "name": "Windows VS2022 Debug",
      "compileCommands": "${workspaceRoot}/compile_commands.json",
      "includePath": [
        "${workspaceRoot}/**",
        "${env.QT_ROOT_DIR}/include",
        "${env.QT_ROOT_DIR}/include/QtCore",
        "${env.QT_ROOT_DIR}/include/QtWidgets",
        "${env.QT_ROOT_DIR}/include/QtGui",
        "${env.QT_ROOT_DIR}/include/QtSvg",
        "${env.QT_ROOT_DIR}/include/QtNetwork",
        "${env.QT_ROOT_DIR}/mkspecs/win32-msvc"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "WIN32",
        "WIN64",
        "_WIN64",
        "_WINDOWS",
        "_ENABLE_EXTENDED_ALIGNED_STORAGE",
        "QT_CORE_LIB",
        "QT_GUI_LIB",
        "QT_WIDGETS_LIB",
        "QT_SVG_LIB",
        "QT_NETWORK_LIB"
      ],
      "intelliSenseMode": "windows-msvc-x64",
      "cStandard": "c17",
      "cppStandard": "c++20"
    }
  ],
  "version": 4
}
