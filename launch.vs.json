{
  "version": "0.2.1",
  "configurations": [
    {
      "type": "cppdbg",
      "name": "CriptoGualet Qt Application (Debug)",
      "request": "launch",
      "program": "${workspaceRoot}/out/build/win-vs2022-x64-debug/bin/Debug/CriptoGualetQt.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}/out/build/win-vs2022-x64-debug/bin/Debug",
      "environment": [
        {
          "name": "PATH",
          "value": "${workspaceRoot}\\out\\build\\win-vs2022-x64-debug\\bin\\Debug;${env:PATH}"
        }
      ],
      "externalConsole": false,
      "MIMode": "lldb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
