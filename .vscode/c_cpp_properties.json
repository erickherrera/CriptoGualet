{
    "configurations": [
        {
            "name": "Windows VS2022 Debug",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compileCommands": "${workspaceFolder}/compile_commands.json",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:QT_ROOT_DIR}/include",
                "${env:QT_ROOT_DIR}/include/QtCore",
                "${env:QT_ROOT_DIR}/include/QtWidgets",
                "${env:QT_ROOT_DIR}/include/QtGui",
                "${env:QT_ROOT_DIR}/include/QtSvg",
                "${env:QT_ROOT_DIR}/include/QtNetwork",
                "${env:QT_ROOT_DIR}/mkspecs/win32-msvc"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "WIN32",
                "WIN64",
                "_WIN64",
                "_WINDOWS",
                "_ENABLE_EXTENDED_ALIGNED_STORAGE",
                "QT_CORE_LIB",
                "QT_GUI_LIB",
                "QT_WIDGETS_LIB",
                "QT_SVG_LIB",
                "QT_NETWORK_LIB"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-msvc-x64",
            "compilerPath": "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/*/bin/Hostx64/x64/cl.exe"
        },
        {
            "name": "Windows ClangCL Debug",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compileCommands": "${workspaceFolder}/compile_commands.json",
            "includePath": [
                "${workspaceFolder}/**",
                "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/MSVC/*/include",
                "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/Llvm/x64/include",
                "C:/Program Files (x86)/Windows Kits/10/Include/*/ucrt",
                "C:/Program Files (x86)/Windows Kits/10/Include/*/um",
                "C:/Program Files (x86)/Windows Kits/10/Include/*/shared",
                "${env:QT_ROOT_DIR}/include",
                "${env:QT_ROOT_DIR}/include/QtCore",
                "${env:QT_ROOT_DIR}/include/QtWidgets",
                "${env:QT_ROOT_DIR}/include/QtGui",
                "${env:QT_ROOT_DIR}/include/QtSvg",
                "${env:QT_ROOT_DIR}/include/QtNetwork",
                "vcpkg_installed/x64-windows/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "WIN32",
                "WIN64",
                "_WIN64",
                "_WINDOWS",
                "_ENABLE_EXTENDED_ALIGNED_STORAGE",
                "QT_CORE_LIB",
                "QT_GUI_LIB",
                "QT_WIDGETS_LIB",
                "QT_SVG_LIB",
                "QT_NETWORK_LIB"
            ],
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-clang-x64",
            "compilerPath": "C:/Program Files/Microsoft Visual Studio/2022/Community/VC/Tools/Llvm/x64/bin/clang-cl.exe"
        }
    ],
    "version": 4
}
